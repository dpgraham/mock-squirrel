{
  "name": "mock-squirrel",
  "version": "1.0.0",
  "description": "Mock a Squirrel server for testing purposes",
  "main": "build/index.js",
  "directories": {
    "lib": "lib"
  },
  "scripts": {
    "test": "mocha test/unit/",
    "test:watch": "mocha --watch test/unit/",
    "test:e2e": "mocha --timeout 6000 test/e2e/",
    "build": "babel lib --out-dir build/",
    "build:watch": "babel lib --out-dir build/ --watch",
    "lint": "eslint lib/ test/"
  },
  "author": "Dan Graham",
  "license": "ISC",
  "devDependencies": {
    "babel": "^6.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^4.18.1",
    "eslint-config-appium": "^2.1.0",
    "request-promise": "^4.2.2",
    "sinon": "^4.4.2"
  },
  "dependencies": {
    "appium-support": "^2.13.0",
    "express": "^4.16.2",
    "lodash": "^4.17.5",
    "semver": "^5.5.0",
    "winston": "^2.4.0"
  }
}
